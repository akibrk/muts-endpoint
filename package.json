  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.23",
    "@types/node": "^11.11.3",
    "codecov": "^3.2.0",
    "nyc": "^13.3.0",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333"
