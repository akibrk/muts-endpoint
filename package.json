{
  "name": "@mu-ts/endpoints",
  "version": "2.0.1",
  "description": "Simple REST endpoint and action routing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "matt.filion@gmail.com",
  "homepage": "https://github.com/mu-ts/rest_endpoints#readme",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "codecov": "mkdir .nyc_output && nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "files": [
    "dist/*",
    "README.md",
    "LICENSE",
    "CODE_OF_CONDUCT.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mu-ts/rest_endpoints.git"
  },
  "keywords": [
    "AWS",
    "API Gateway",
    "rest",
    "endpoints",
    "http"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jeremyosborne/packagejsonexample/blob/master/LICENSE.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/mu-ts/rest_endpoints/issues"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.13.14",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.5.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@mu-ts/logger": "^2.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  }
}
